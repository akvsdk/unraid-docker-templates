<?xml version="1.0"?>
<Container>
    <Name>aliyundrive-webdav</Name>
    <Repository>docker.mirrors.ustc.edu.cn/messense/aliyundrive-webdav</Repository>
    <Registry>https://hub.docker.com/r/messense/aliyundrive-webdav</Registry>
    <Network>bridge</Network>
    <Privileged>false</Privileged>
    <Support>https://github.com/messense/aliyundrive-webdav/issues</Support>
    <Project>https://github.com/messense/aliyundrive-webdav</Project>
    <Overview>aliyundrive-webdav</Overview>
    <Description>
        阿里云webdav客户端的docker镜像
    </Description>
    <Icon>https://pan.baidu.com/m-static/base/static/images/favicon.ico</Icon>
    <Data>
        <Volume>
            <HostDir>/mnt/user/Docker/aliyundrive-webdav</HostDir>
            <ContainerDir>/etc/aliyundrive-webdav</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <Environment>
        <Variable>
            <Value>J1ang</Value>
            <Name>WEBDAV_AUTH_USER</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>66351579</Value>
            <Name>WEBDAV_AUTH_PASSWORD</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>99</Value>
            <Name>PUID</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>100</Value>
            <Name>PGID</Name>
            <Mode/>
        </Variable>
    </Environment>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>6082</HostPort>
                <ContainerPort>6080</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
</Container>
