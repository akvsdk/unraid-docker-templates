<?xml version="1.0"?>
<Container>
    <Name>aliyundrive-webdav</Name>
    <Repository>docker.mirrors.ustc.edu.cn/messense/aliyundrive-webdav</Repository>
    <Registry>https://hub.docker.com/r/messense/aliyundrive-webdav</Registry>
    <Network>bridge</Network>
    <Privileged>false</Privileged>
    <Support>https://github.com/messense/aliyundrive-webdav/issues</Support>
    <Project>https://github.com/messense/aliyundrive-webdav</Project>
    <Overview>aliyundrive-webdav</Overview>
    <Description>
        阿里云webdav客户端的docker镜像
    </Description>
    <Icon>https://files.catbox.moe/3c82ad.jpeg</Icon>
    <Data>
        <Volume>
            <HostDir>/mnt/user/Docker/aliyundrive-webdav</HostDir>
            <ContainerDir>/etc/aliyundrive-webdav</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <Environment>
        <Variable>
            <Value>5c0e638a92ee4f80805f0aa83f497763</Value>
            <Name>REFRESH_TOKEN</Name>
            <Description>阿里云盘Token</Description>
            <Mode/>
        </Variable>
        <Variable>
            <Value>J1ang</Value>
            <Name>WEBDAV_AUTH_USER</Name>
            <Description>webdav用户名</Description>
            <Mode/>
        </Variable>
        <Variable>
            <Value>66351579</Value>
            <Name>WEBDAV_AUTH_PASSWORD</Name>
             <Description>webdav密码</Description>
            <Mode/>
        </Variable>
        <Variable>
            <Value>99</Value>
            <Name>PUID</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value>100</Value>
            <Name>PGID</Name>
            <Mode/>
        </Variable>
    </Environment>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>5223</HostPort>
                <ContainerPort>8080</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
</Container>
